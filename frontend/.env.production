# Frontend Production Environment Configuration
# Social Networks Panel - Real API Mode

# Mock Mode Configuration
REACT_APP_ENABLE_MOCK_MODE=false

# API Configuration (production backend)
REACT_APP_API_URL=https://api.roastr.ai

# Supabase Configuration (production values)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-production-anon-key

# Feature Flags
REACT_APP_ENABLE_SOCIAL_PANEL=true
REACT_APP_ENABLE_AUTH_SYSTEM=true
REACT_APP_ENABLE_DEBUG_LOGS=false

# Production Settings
GENERATE_SOURCEMAP=false
REACT_APP_ENV=production

# Analytics and Monitoring
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_SENTRY_DSN=your-sentry-dsn
